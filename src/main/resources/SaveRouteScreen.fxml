      <GridPane.margin>
        <Insets />
      </GridPane.margin>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
    </Button>
    <Label fx:id="routeNameLabel" text="Route name:" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.valignment="TOP">
         <font>
            <Font name="KacstOne Bold" size="13.0" />
         </font></Label>
    <TextField fx:id="routeNameText" prefWidth="200.0" style="-fx-background-color: #0B4D63, #CCE6EF; -fx-background-insets: 0,1.5;" GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.valignment="TOP" />
    <Button fx:id="saveButton" mnemonicParsing="false" onAction="#savePressed" prefHeight="22.0" prefWidth="80.0" style="-fx-background-color: #30126D, #B39EDC;" text="Save" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="BOTTOM">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <GridPane.margin>
            <Insets />
         </GridPane.margin></Button>
      <TextField fx:id="routeStartText" prefWidth="200.0" style="-fx-background-color: #0B4D63, #CCE6EF; -fx-background-insets: 0,1.5;" GridPane.columnIndex="1" GridPane.valignment="CENTER">
         <GridPane.margin>
            <Insets bottom="30.0" />
         </GridPane.margin>
      </TextField>
      <Label fx:id="routeStartLabel" prefHeight="56.0" prefWidth="196.0" text="Where does this route start?" wrapText="true" GridPane.valignment="CENTER">
         <font>
            <Font name="KacstOne Bold" size="13.0" />
         </font>
         <GridPane.margin>
            <Insets bottom="20.0" />
         </GridPane.margin>
      </Label>
      <Label fx:id="routeEndLabel" prefHeight="56.0" prefWidth="196.0" text="Where does this route end?" wrapText="true" GridPane.valignment="BOTTOM">
         <font>
            <Font name="KacstOne Bold" size="13.0" />
